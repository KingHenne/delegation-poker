{
  "name": "delegation-poker",
  "version": "0.1.0",
  "homepage": "http://hendrik-liebau.de/delegation-poker",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-require-ignore": "^0.1.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "flow-bin": "^0.41.0",
    "ftp-deploy": "^1.1.0",
    "husky": "^0.13.2",
    "lint-staged": "^3.4.0",
    "prettier": "^0.22.0",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "prerender": "BABEL_ENV=node babel-node ./scripts/prerender",
    "postbuild": "npm run prerender",
    "deploy": "BABEL_ENV=node babel-node ./scripts/deploy",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma all --parser flow --write '{src,scripts}/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "babel": {
    "env": {
      "node": {
        "presets": [
          "es2015",
          "react"
        ],
        "plugins": [
          [
            "babel-plugin-transform-require-ignore",
            {
              "extensions": [
                ".css"
              ]
            }
          ]
        ]
      }
    }
  }
}
